{
  "name": "ride-generator",
  "version": "0.1.2",
  "private": false,
  "engines": {
    "node": "14.x",
    "npm": "8.1.x"
  },
  "keywords": [
    "vue",
    "vue.js",
    "geojson",
    "turf.js",
    "graphhopper",
    "jawgmaps",
    "map",
    "maplibre",
    "maplibre-gl-js"
  ],
  "author": "superrache",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/superrache/ride-generator/issues"
  },
  "homepage": "https://github.com/superrache/ride-generator",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.prod.js",
    "release": "standard-version"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "maplibre-gl": "^1.15.2",
    "serve-static": "^1.14.1",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
